package com.cncmes.dto;

public class CNCScheduler {
	private int id;
	private String ip;
	private String portmachine;
	private String portrobot;
	private String portmaterial;
	private String porttask;
	private String portrack;
	private String realTableName = "cnc_scheduler";
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getIp() {
		return ip;
	}
	public void setIp(String ip) {
		this.ip = ip;
	}
	public String getPortmachine() {
		return portmachine;
	}
	public void setPortmachine(String portmachine) {
		this.portmachine = portmachine;
	}
	public String getPortrobot() {
		return portrobot;
	}
	public void setPortrobot(String portrobot) {
		this.portrobot = portrobot;
	}
	public String getPortmaterial() {
		return portmaterial;
	}
	public void setPortmaterial(String portmaterial) {
		this.portmaterial = portmaterial;
	}
	public String getPorttask() {
		return porttask;
	}
	public void setPorttask(String porttask) {
		this.porttask = porttask;
	}
	public String getPortrack() {
		return portrack;
	}
	public void setPortrack(String portrack) {
		this.portrack = portrack;
	}
	public String getRealTableName() {
		return realTableName;
	}
	public void setRealTableName(String realTableName) {
		this.realTableName = realTableName;
	}
	@Override
	public String toString() {
		return "CNCScheduler [id=" + id + ", ip=" + ip + ", portmachine=" + portmachine + ", portrobot=" + portrobot
				+ ", portmaterial=" + portmaterial + ", porttask=" + porttask + ", portrack=" + portrack
				+ ", realTableName=" + realTableName + "]";
	}
	public CNCScheduler(int id, String ip, String portmachine, String portrobot, String portmaterial, String porttask,
			String portrack, String realTableName) {
		super();
		this.id = id;
		this.ip = ip;
		this.portmachine = portmachine;
		this.portrobot = portrobot;
		this.portmaterial = portmaterial;
		this.porttask = porttask;
		this.portrack = portrack;
		this.realTableName = realTableName;
	}
	public CNCScheduler() {
		super();
	}
}

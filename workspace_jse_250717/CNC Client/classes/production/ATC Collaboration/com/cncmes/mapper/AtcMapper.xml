<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace=绑定一个对应的Dao/Mapper接口-->
<mapper namespace="com.cncmes.dao.AtcDao">
    <!--select查询语句-->
    <select id="getAllATC" resultType="com.cncmes.dto.ATC">
        select * from atc where is_delete = 0
    </select>
</mapper>
